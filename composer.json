{
    "name": "al-one/laravel-predis-listened",
    "description": "Listening Laravel pRedis Queries",
    "keywords": ["laravel", "predis"],
    "authors": [
        {
            "name": "Alone",
            "email": "hi@anlo.ng"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.8.30 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8.30 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/redis": "^5.8.30 || ^6.0 || ^7.0 || ^8.0",
        "predis/predis": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Alone\\LaravelPredisListened\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alone\\LaravelPredisListened\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
