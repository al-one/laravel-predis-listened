{
    "name": "al-one/laravel-predis-listened",
    "description": "Listening Laravel pRedis Queries",
    "keywords": ["laravel", "predis"],
    "authors": [
        {
            "name": "Alone",
            "email": "hi@anlo.ng"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/al-one/laravel-redis-extendable.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "al-one/laravel-redis-extendable": "~1.5",
        "illuminate/contracts": "~5.4",
        "illuminate/support": "~5.4",
        "illuminate/redis": "~5.4",
        "predis/predis": "~1.0"
    },

    "autoload": {
        "psr-4": {
            "Alone\\LaravelPredisListened\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alone\\LaravelPredisListened\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
